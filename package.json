{
  "name": "general-store-crud",
  "version": "1.0.0",
  "description": "C.R.U.D. actions for general-store.",
  "main": "lib/GeneralStoreCRUD.js",
  "scripts": {
    "build:commonjs": "BABEL_ENV=commonjs babel src --ignore *-test.js --out-dir lib",
    "build:umd": "NODE_ENV=development webpack --config ./webpack.config.development.js src/GeneralStoreCRUD.js dist/general-store-crud.js",
    "build:umd:min": "NODE_ENV=production webpack --config ./webpack.config.production.js -p src/GeneralStoreCRUD.js dist/general-store-crud.min.js",
    "build": "npm run build:commonjs && npm run build:umd && npm run build:umd:min",
    "build-and-test": "npm run clean && npm run build && npm test",
    "check": "flow check",
    "clean": "rm -rf ./dist && rm -rf lib",
    "lint": "eslint ./src",
    "prepublish": "npm run build-and-test",
    "test": "npm run check && npm run lint && jest"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-jest": "^13.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "eslint": "^3.0.0",
    "eslint-config-hubspot": "^6.2.0",
    "eslint-plugin-react": "^5.2.2",
    "flow-bin": "^0.28.0",
    "invariant": "^2.2.1",
    "jasmine-check": "^0.1.5",
    "jest-cli": "^13.2.3",
    "webpack": "^1.13.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/colbyr/general-store-crud.git"
  },
  "keywords": [
    "react",
    "flux",
    "general-store",
    "rest"
  ],
  "author": "Colby Rabideau <me@colbyr.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/colbyr/general-store-crud/issues"
  },
  "homepage": "https://github.com/colbyr/general-store-crud#readme",
  "jest": {
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "unmockedModulePathPatterns": [
      "invariant"
    ]
  }
}
